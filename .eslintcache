language/language.provider';\nimport Product, { ProductProps } from '../../../components/Products';\ninterface Props {\n   value: string;\n   category?: string;\n}\n\nconst ProductList: React.FC = () => {\n   const {\n      state: { query, category },\n   } = useSearch();\n\n   const {\n      state: { userLanguage },\n   } = useLanguage();\n\n   const history = useHistory<null | undefined | Props>();\n   const targetRef = useRef<HTMLDivElement | null>(null);\n   const { data, fetchMore } = useQuery(GET_PRODUCT_DETAILS, {\n      variables: {\n         limit: 20,\n         offset: 0,\n         text: history.location ? history?.location?.state?.value : '',\n         category: history.location ? history?.location?.state?.category : '',\n      },\n   });\n\n   useEffect(() => {\n      const target = targetRef.current;\n      if (\n         history?.location.state &&\n         (query.length > 0 || category !== undefined)\n      ) {\n         window.scrollTo({\n            top: target!.offsetHeight,\n            behavior: 'smooth',\n         });\n      }\n   }, [query, category]);\n\n   const scheme = 'green';\n   const step2 = useColorModeValue('500', '700');\n   const step3 = useColorModeValue('300', '500');\n\n   const loadMore = () => {\n      fetchMore({\n         variables: {\n            limit: 8,\n            offset: Number(data.products.items.length),\n         },\n         updateQuery: (prev, { fetchMoreResult }) => {\n            if (!fetchMoreResult) {\n               return prev;\n            }\n            return {\n               products: {\n                  __typename: prev.products.__typename,\n                  items: [\n                     ...prev.products.items,\n                     ...fetchMoreResult.products.items,\n                  ],\n                  hasMore: fetchMoreResult.products.hasMore,\n               },\n            };\n         },\n      });\n   };\n   return (\n      <div ref={targetRef}>\n         <ProductGrid>\n            {data?.products?.items?.map(\n               (items: ProductProps, index: number) => (\n                  <Product\n                     key={index}\n                     userLanguage={userLanguage}\n                     image={items.image}\n                     price={items.price}\n                     title={items.title}\n                     description={items.description}\n                     id={items.id}\n                     unit={items.unit}\n                  />\n               )\n            )}\n         </ProductGrid>\n         {data?.products?.hasMore && (\n            <Flex pb=\"20px\" justify=\"center\">\n               <Button\n                  backgroundColor=\"white\"\n                  color=\"black.blackAlpha.200\"\n                  fontWeight=\"400\"\n                  fontSize=\"14px\"\n                  size={'md'}\n                  rounded={'md'}\n                  _focus={{\n                     outline: 'none',\n                  }}\n                  fontFamily=\"Lato , sans-serif\"\n                  transition=\"background 0.8s\"\n                  backgroundPosition=\"center\"\n                  _hover={{\n                     bgColor: `${scheme}.${step2}`,\n                     bgGradient: `radial(circle, transparent 1%, ${scheme}.${step2} 1%)`,\n                     bgPos: 'center',\n                     color: 'white',\n                     backgroundSize: '15000%',\n                  }}\n                  _active={{\n                     bgColor: `${scheme}.${step3}`,\n                     backgroundSize: '100%',\n                     transition: 'background 0s',\n                  }}\n                  margin=\"0px\"\n                  border=\"0px\"\n                  flexShrink={0}\n                  cursor=\"pointer\"\n                  outline=\"0px\"\n                  paddingLeft=\"30px\"\n                  paddingRight=\"30px\"\n                  onClick={loadMore}\n               >\n                  Load More\n               </Button>\n            </Flex>\n         )}\n      </div>\n   );\n};\n\nexport default React.memo(ProductList);\n","/home/dibash/react-ecommerce/src/components/FormModal/index.tsx",[],"/home/dibash/react-ecommerce/src/components/Sidebar/index.tsx",[],"/home/dibash/react-ecommerce/src/graphql/query/product.query.ts",[],"/home/dibash/react-ecommerce/src/components/Sidebar/AllSvgIcon.tsx",[],"/home/dibash/react-ecommerce/src/graphql/query/category.query.ts",[],"/home/dibash/react-ecommerce/src/contexts/search/search.provider.tsx",[],"/home/dibash/react-ecommerce/src/contexts/search/search.reducer.ts",[],"/home/dibash/react-ecommerce/src/contexts/search/search.context.tsx",[],"/home/dibash/react-ecommerce/src/components/SearchBox/index.tsx",["196"],"/home/dibash/react-ecommerce/src/components/SearchBox/index.style.tsx",[],"/home/dibash/react-ecommerce/src/pages/Home/Screens/Search.style.tsx",[],"/home/dibash/react-ecommerce/src/components/Button/index.tsx",[],"/home/dibash/react-ecommerce/src/pages/Home/Screens/ProductList.style.tsx",[],"/home/dibash/react-ecommerce/src/layouts/RouteLayout/index.tsx",[],"/home/dibash/react-ecommerce/src/themes.ts",[],"/home/dibash/react-ecommerce/src/components/Popover/index.tsx",[],["197","198"],"/home/dibash/react-ecommerce/src/components/Popover/index.style.tsx",[],"/home/dibash/react-ecommerce/src/contexts/language/language.provider.tsx",[],"/home/dibash/react-ecommerce/src/contexts/language/language.context.tsx",[],"/home/dibash/react-ecommerce/src/contexts/language/language.reducer.ts",[],"/home/dibash/react-ecommerce/src/languages/index.ts",[],"/home/dibash/react-ecommerce/src/components/Navbar/index.style.tsx",[],"/home/dibash/react-ecommerce/src/components/Products/index.tsx",[],"/home/dibash/react-ecommerce/src/components/Products/index.style.tsx",[],"/home/dibash/react-ecommerce/src/components/Cart/index.tsx",[],"/home/dibash/react-ecommerce/src/components/Cart/index.style.tsx",[],"/home/dibash/react-ecommerce/src/components/P